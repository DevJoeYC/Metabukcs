package meta.test;

import meta.dto.productsDTO;
import meta.vo.productsVO;

public class testMenuList {

	public static void main(String[] args)
	{
		productsDTO	dto = new productsDTO();
		productsVO	pro = null;
		
		//메뉴 리스트 생성 / 예) 판매중인 상품의 중분류
		dto.GetMenuList();
		//상품 리스트 생성 / 예) 판매중인 상품 전체 리스트
		dto.GetProList();
		
		//리스트 갯수 산출
		int MAX = dto.GetTotal("menu");
		System.out.println(Max);
		dto.GetTotal("pro");
		
		
		pro = dto.GetMenuItem(0);
		pro.getInfo("menu");
		System.out.println("==========================================================");
		pro = dto.GetMenuItem(1);
		pro.getInfo("menu");
		System.out.println("==========================================================");
		pro = dto.GetProItem(0);
		pro.getInfo("pro");
		System.out.println("==========================================================");
		pro = dto.GetProItem(1);
		pro.getInfo("pro");
		System.out.println("==========================================================");
		
	}

}
