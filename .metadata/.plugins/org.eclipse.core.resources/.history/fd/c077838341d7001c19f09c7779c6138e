package meta.test;

import meta.dto.productsDTO;
import meta.vo.productsVO;

public class testMenuList {

	public static void main(String[] args)
	{
		productsDTO	dto = new productsDTO();
		productsVO	pro = null;
		
		pro = dto.GetMenuItem(0);
		pro.getInfo("menu");
		System.out.println(pro.name());
		System.out.println(pro.hname());
		System.out.println(pro.ename());
		System.out.println(pro.price());
		System.out.println(pro.flag_sell());
		System.out.println("==========================================================");
		pro = dto.GetMenuItem(1);
		pro.getInfo("menu");
		System.out.println(pro.name());
		System.out.println(pro.hname());
		System.out.println(pro.ename());
		System.out.println(pro.price());
		System.out.println(pro.flag_sell());
		System.out.println("==========================================================");
		pro = dto.GetProItem(0);
		pro.getInfo("pro");
		System.out.println(pro.name());
		System.out.println(pro.hname());
		System.out.println(pro.ename());
		System.out.println(pro.price());
		System.out.println(pro.flag_sell());
		System.out.println("==========================================================");
		pro = dto.GetProItem(1);
		pro.getInfo("pro");
		System.out.println(pro.name());
		System.out.println(pro.hname());
		System.out.println(pro.ename());
		System.out.println(pro.price());
		System.out.println(pro.flag_sell());
		System.out.println("==========================================================");
		
	}

}
