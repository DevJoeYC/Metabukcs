	private	int		sp_inven;							//	sp_inven	from	sale_product	재고량

	//추가 정보
	private	int		pro_uprice;							//	pro_uprice	from	st_product		단가
	private ArrayList<nutriVO>	nutriList;				//상품 이미지 객체
	private	imageVO[]	images;							//상품 이미지 객체 배열
	private	int		def_rcpv;							//레시피개수
	private ArrayList<recipeVO>	reciList;				//상품 레시피 객체

	public	int		getPro_no()		{ return pro_no;	}			// 	pro_no		from	st_product		매장상품번호
	public	String	getName()		{ return name;		}			// 	div_name	from	div_product		세부상품명
	public	String	getHname()		{ return hname;		}			// 	def_hname	from	def_product		상품명(한글)
	public	String	getEname()		{ return ename;		}			// 	def_ename	from	def_product		상품명(영문)
	public	String	getCat()		{ return cat;		}			// 	cat_name	from	category		카테고리명
	public	String	getInfo()		{ return info;		}			// 	def_info	from	def_product		상품설명
	public	int		getVolume()		{ return volume;	}			// 	volume		from	div_product		용량
	public	String	getUnit()		{ return unit;		}			// 	unit		from	div_product		단위
	public	int		getPrice()		{ return price;		}			// 	pro_price	from	st_product		판매가
	public	String	getAcc()		{ return acc;		}			// 	pro_acc		from	st_product		적립률
	public	String	getFlag_recom()	{ return flag_recom;}			// 	pro_recom	from	st_product		추천여부
	public	String	getFlag_sell()	{ return flag_sell;	}			// 	pro_sell	from	st_product		판매여부
	public	String	getFlag_rec()	{ return flag_rec;	}			// 	pro_chrcp	from	st_product		레시피변경유무
	public	int		getSp_no()		{ return sp_no;		}			// 	sp_no		from	sale_product	판매상품번호
	public	int		getSp_count()	{ return sp_count;	}			// 	sp_count	from	sale_product	판매가능수량
	public	int		getSp_inven()	{ return sp_inven;	}			// 	sp_inven	from	sale_product	재고량
	public	int		getPro_uprice()	{ return pro_uprice;}			// 	pro_uprice	from	st_product		단가
	public	ArrayList<nutriVO> getNutriList() { return nutriList;}	//상품 이미지 객체
	public	imageVO[] getImages()	{ return images;	}			//상품 이미지 객체 배열
	public	int		getDef_rcpv()	{ return def_rcpv;	}			//레시피개수
	public	ArrayList<recipeVO> getReciList() { return reciList;}	//상품 레시피 객체

	public	void setPro_no(		int		pro_no		) { this.pro_no		 = pro_no;		}		//	pro_no		from	st_product		매장상품번호
	public	void setName(		String	name		) { this.name		 = name;		}		//	div_name	from	div_product		세부상품명
	public	void setHname(		String	hname		) { this.hname		 = hname;		}		//	def_hname	from	def_product		상품명(한글)
	public	void setEname(		String	ename		) { this.ename		 = ename;		}		//	def_ename	from	def_product		상품명(영문)
	public	void setCat(		String	cat			) { this.cat		 = cat;			}		//	cat_name	from	category		카테고리명
	public	void setInfo(		String	info		) { this.info		 = info;		}		//	def_info	from	def_product		상품설명
	public	void setVolume(		int		volume		) { this.volume		 = volume;		}		//	volume		from	div_product		용량
	public	void setUnit(		String	unit		) { this.unit		 = unit;		}		//	unit		from	div_product		단위
	public	void setPrice(		int		price		) { this.price		 = price;		}		//	pro_price	from	st_product		판매가
	public	void setAcc(		String	acc			) { this.acc		 = acc;			}		//	pro_acc		from	st_product		적립률
	public	void setFlag_recom(	String	flag_recom	) { this.flag_recom	 = flag_recom;	}		//	pro_recom	from	st_product		추천여부
	public	void setFlag_sell(	String	flag_sell	) { this.flag_sell	 = flag_sell;	}		//	pro_sell	from	st_product		판매여부
	public	void setFlag_rec(	String	flag_rec	) { this.flag_rec	 = flag_rec;	}		//	pro_chrcp	from	st_product		레시피변경유무
	public	void setSp_no(		int		sp_no		) { this.sp_no		 = sp_no;		}		//	sp_no		from	sale_product	판매상품번호
	public	void setSp_count(	int		sp_count	) { this.sp_count	 = sp_count;	}		//	sp_count	from	sale_product	판매가능수량
	public	void setSp_inven(	int		sp_inven	) { this.sp_inven	 = sp_inven;	}		//	sp_inven	from	sale_product	재고량
	public	void setPro_uprice(	int		pro_uprice	) { this.pro_uprice	 = pro_uprice;	}		//	pro_uprice	from	st_product		단가
	public	void setNutriList(	ArrayList<nutriVO> nutriList) { this.nutriList = nutriList;	}	//상품 이미지 객체
	public	void setImages(		imageVO[] images	) { this.images		 = images;		}		//상품 이미지 객체 배열
	public	void setDef_rcpv(	int def_rcpv		) { this.def_rcpv	 = def_rcpv;	}		//레시피개수
	public	void setReciList(	ArrayList<recipeVO> reciList) { this.reciList = reciList;	}	//상품 레시피 객체
}